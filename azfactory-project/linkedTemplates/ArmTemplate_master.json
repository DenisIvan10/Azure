{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azfactory-project"
		},
		"datalakestorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalakestorage'"
		},
		"datalakestorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azfystore.dfs.core.windows.net/"
		},
		"httplinkedservice_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azfactory-project_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"datalakestorage_accountKey": {
						"value": "[parameters('datalakestorage_accountKey')]"
					},
					"datalakestorage_properties_typeProperties_url": {
						"value": "[parameters('datalakestorage_properties_typeProperties_url')]"
					},
					"httplinkedservice_properties_typeProperties_url": {
						"value": "[parameters('httplinkedservice_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}